    "titanium-sdk",
    "titanium-mobile",
    "titanium-module",
    "axway-sdk",
    "axway-amplify",
    "brenton-house"
  ],
  "devDependencies": {
    "@titanium/eslint-config": "^1.0.8",
    "hashids": "^2.2.10"
  }
}
